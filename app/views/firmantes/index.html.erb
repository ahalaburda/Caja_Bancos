<h1>Listing firmantes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @firmantes.each do |firmante| %>
      <tr>
        <td><%= firmante.nombre %></td>
        <td><%= link_to 'Show', firmante %></td>
        <td><%= link_to 'Edit', edit_firmante_path(firmante) %></td>
        <td><%= link_to 'Destroy', firmante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Firmante', new_firmante_path %>
