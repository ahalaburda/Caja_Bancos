<h1> Cheques entrantes <button type="button" class="btn btn-info"><span class="glyphicon glyphicon-plus"></span></button></h1>
<div id="form1">
  <%= render 'form' %>
</div>
<table id="table">
  <thead>
    <tr>
      <th>Id banco</th>
      <th>Numero</th>
      <th>Monto</th>
      <th>Concepto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cheques_entrantes.each do |cheques_entrante| %>
      <tr>
        <td><%= cheques_entrante.id_banco %></td>
        <td><%= cheques_entrante.numero %></td>
        <td><%= cheques_entrante.monto %></td>
        <td><%= cheques_entrante.concepto %></td>
        <td><%= link_to 'Show', cheques_entrante %></td>
        <td><%= link_to 'Edit', edit_cheques_entrante_path(cheques_entrante) %></td>
        <td><%= link_to 'Destroy', cheques_entrante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cheques entrante', new_cheques_entrante_path %>
