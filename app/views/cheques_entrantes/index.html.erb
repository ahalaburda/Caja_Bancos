<h1> Cheques entrantes <button type="button" class="btn btn-info btn-outline btn-circle" id="btn1"><span class="glyphicon glyphicon-chevron-up" id="gbtn1"></span></button></h1>
<div id="form1">
  <%= render 'form' %>
</div>
<table id="table">
  <thead>
    <tr>
      <th>Id banco</th>
      <th>Numero</th>
      <th>Monto</th>
      <th>Concepto</th>
      <th>Acciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cheques_entrantes.each do |cheques_entrante| %>
      <tr>
        <td><%= cheques_entrante.id_banco %></td>
        <td><%= cheques_entrante.numero %></td>
        <td><%= cheques_entrante.monto %></td>
        <td><%= cheques_entrante.concepto %></td>
        <td>
           <!--  <%= link_to caja , class: 'btn btn-info btn-outline btn-circle', title: 'View', 'data-toggle' => 'tooltip', 'data-placement' => 'right' do %>
                <span class="glyphicon glyphicon-search"></span>
                <% end %> -->
            
            <%= link_to edit_caja_path(caja), class: 'btn btn-warning btn-outline btn-circle', title: 'Editar', 'data-toggle' => 'tooltip', 'data-placement' => 'right' do %>
                <span class="glyphicon glyphicon-pencil"></span>
                <% end %>
            
            <%= link_to caja, method: :delete, data: { confirm: 'Va eliminarlo, esta seguro?' }, class: 'btn btn-danger btn-outline btn-circle', title: 'Borrar', 'data-toggle' => 'tooltip', 'data-placement' => 'right'  do %>
            <span class="glyphicon glyphicon-remove"></span>
          <% end %>
      </td>
      </tr>
    <% end %>
  </tbody>
</table>
