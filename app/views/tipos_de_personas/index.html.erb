<h1>Listing tipos_de_personas</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_de_personas.each do |tipos_de_persona| %>
      <tr>
        <td><%= tipos_de_persona.descripcion %></td>
        <td><%= link_to 'Show', tipos_de_persona %></td>
        <td><%= link_to 'Edit', edit_tipos_de_persona_path(tipos_de_persona) %></td>
        <td><%= link_to 'Destroy', tipos_de_persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipos de persona', new_tipos_de_persona_path %>
