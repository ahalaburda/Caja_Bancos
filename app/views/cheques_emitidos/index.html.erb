<h1> Cheques Emitidos <button type="button" class="btn btn-info btn-outline btn-circle" id="btn1"><span class="glyphicon glyphicon-chevron-up" id="gbtn1"></span></button></h1>
<div id="form1">
  <%= render 'form' %>
</div>
<table id="table">
  <thead>
    <tr>
      <th>Id cuenta bancaria</th>
      <th>Id banco</th>
      <th>Fecha</th>
      <th>Concepto</th>
      <th>Monto</th>
      <th>Acciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cheques_emitidos.each do |cheques_emitido| %>
      <tr>
        <td><%= cheques_emitido.id_cuenta_bancaria %></td>
        <td><%= cheques_emitido.id_banco %></td>
        <td><%= cheques_emitido.fecha %></td>
        <td><%= cheques_emitido.concepto %></td>
        <td><%= cheques_emitido.monto %></td>
        <td>
           <!--  <%= link_to caja , class: 'btn btn-info btn-outline btn-circle', title: 'View', 'data-toggle' => 'tooltip', 'data-placement' => 'right' do %>
                <span class="glyphicon glyphicon-search"></span>
                <% end %> -->
            
            <%= link_to edit_caja_path(caja), class: 'btn btn-warning btn-outline btn-circle', title: 'Editar', 'data-toggle' => 'tooltip', 'data-placement' => 'right' do %>
                <span class="glyphicon glyphicon-pencil"></span>
                <% end %>
            
            <%= link_to caja, method: :delete, data: { confirm: 'Va eliminarlo, esta seguro?' }, class: 'btn btn-danger btn-outline btn-circle', title: 'Borrar', 'data-toggle' => 'tooltip', 'data-placement' => 'right'  do %>
            <span class="glyphicon glyphicon-remove"></span>
          <% end %>
      </td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to "PRINT", '#', onclick: "printpage()"%> | 
  <%= link_to 'PDF', cheques_emitidos_path(@cheques_emitidos, format: 'pdf') %> | 
  <%= link_to 'Excel Download', cheques_emitidos_path(:format => :xls) %>
</table>