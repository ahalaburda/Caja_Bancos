<h1> Tipos de Estados <button type="button" class="btn btn-info" id="btn1"><span class="glyphicon glyphicon-chevron-up" id="gbtn1"></span></button></h1>
<div id="form1">
  <%= render 'form' %>
</div>
<table id="table">
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_de_estados.each do |tipos_de_estado| %>
      <tr>
        <td><%= tipos_de_estado.descripcion %></td>
        <td><%= link_to 'Show', tipos_de_estado %></td>
        <td><%= link_to 'Edit', edit_tipos_de_estado_path(tipos_de_estado) %></td>
        <td><%= link_to 'Destroy', tipos_de_estado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipos de estado', new_tipos_de_estado_path %>
